<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	 <class name="com.persistent.MainSetting" table="jd_main_setting">
        <id name="msId" type="java.lang.Integer">
            <column name="ms_id" />
            <generator class="native" />
        </id>
        <property name="msDocNo" type="java.lang.String">
            <column name="ms_doc_no"  length="150"/>
        </property>
        <property name="msApproval2" type="java.lang.Integer">
            <column name="ms_approval_2" length="10" />
        </property>
		<property name="msNoApproval" type="java.lang.Integer">
            <column name="ms_no_approval" length="10" />
        </property>
         <property name="msRecruitEmail" type="java.lang.String">
            <column name="ms_recruit_email"  length="230"/>
        </property>
        <property name="msDocDate" type="java.util.Date">
            <column name="ms_doc_date" length="0" />
        </property>
        <property name="msDocVersion" type="java.lang.String">
            <column name="ms_doc_version"  length="150"/>
        </property>
	</class>

</hibernate-mapping>